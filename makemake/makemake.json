{
    "targets": [
        {
            "type": "executable",
            "name": "makemake",
            "cc": "gcc",
            "cxx": "g++",
            "cflags": "-O2 -W -Wall",
            "cxxflags": "-O2 -W -Wall -Wextra -Iinclude -std=c++17",
            "ar": "ar",
            "arflags": "rcs",
            "libs": "-lstdc++fs",
            "install": "install -D -m0755 makemake /usr/bin",
